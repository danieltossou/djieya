<p id="notice"><%= notice %></p>

<h1>Annees</h1>

<table>
  <thead>
    <tr>
      <th>Libelle</th>
      <th>Etat</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @annees.each do |annee| %>
      <tr>
        <td><%= annee.libelle %></td>
        <td><%= annee.etat %></td>
        <td><%= annee.admin_id %></td>
        <td><%= link_to 'Show', annee %></td>
        <td><%= link_to 'Edit', edit_annee_path(annee) %></td>
        <td><%= link_to 'Destroy', annee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Annee', new_annee_path %>
