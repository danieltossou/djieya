<p id="notice"><%= notice %></p>

<div class="container">
  <div class="row justify-content-md-center">
    <div class="card">
      <div class="card-header">
      <h2 class="text-center">LISTE DES ANNEES</h2>
      </div>
      <div class="card-body">
          <table class="table table-hover">
              <thead>
                <tr>
                  <th>Libelle</th>
                  <th>Etat</th>
                  <th>User</th>
                  <th colspan="3"></th>
                </tr>
              </thead>
            
              <tbody>
                <% @annees.each do |annee| %>
                  <tr>
                    <td><%= annee.libelle %></td>
                    <td><%= annee.etat %></td>
                    <td><%= annee.user_id %></td>
                    <td><%= link_to 'Show', annee %></td>
                    <td><%= link_to 'Edit', edit_annee_path(annee) %></td>
                    <td><%= link_to 'Destroy', annee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                  </tr>
                <% end %>
              </tbody>
            </table>
      </div>
      <div class="card-footer">
          <%= link_to 'RETOUR', :back, :class => 'btn btn-default' %>
      </div>
    </div>
  </div>
</div>