<%= form_with(model: annee, local: true) do |form| %>


  <div class="field">
    <%= form.label :Libelle %>
    <%= form.text_field :libelle, :class => 'form-control' %>
  </div>

  <div class="field">
    <%= form.label :Etat %>
    <%= form.check_box :etat %>
  </div>

  <div class="actions">
    <%= form.submit "VALIDER", :class => 'btn btn-primary' %>
  </div>
<% end %>