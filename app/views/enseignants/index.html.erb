<p id="notice"><%= notice %></p>

<h1>Enseignants</h1>

<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th>Prenom</th>
      <th>Contact</th>
      <th>Email</th>
      <th>Adresse</th>
      <th>Ecole</th>
      <th>User</th>
      <th>Admin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enseignants.each do |enseignant| %>
      <tr>
        <td><%= enseignant.nom %></td>
        <td><%= enseignant.prenom %></td>
        <td><%= enseignant.contact %></td>
        <td><%= enseignant.email %></td>
        <td><%= enseignant.adresse %></td>
        <td><%= enseignant.ecole_id %></td>
        <td><%= enseignant.user_id %></td>
        <td><%= enseignant.admin_id %></td>
        <td><%= link_to 'Show', enseignant %></td>
        <td><%= link_to 'Edit', edit_enseignant_path(enseignant) %></td>
        <td><%= link_to 'Destroy', enseignant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enseignant', new_enseignant_path %>
